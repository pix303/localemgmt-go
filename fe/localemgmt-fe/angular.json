{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "localemgmt-fe": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular/build:application",
           "options": {
             "browser": "src/main.ts",
             "tsConfig": "tsconfig.app.json",
             "outputPath": "../dist/",
             "assets": [
               {
                 "glob": "**/*",
                 "input": "public"
               }
             ],
             "styles": [
               "src/styles.css"
             ]
           },
           "configurations": {
             "production": {
               "baseHref": "/fe/",
               "deployUrl":"/fe/",
               "budgets": [
                 {
                   "type": "initial",
                   "maximumWarning": "500kB",
                   "maximumError": "1MB"
                 },
                 {
                   "type": "anyComponentStyle",
                   "maximumWarning": "4kB",
                   "maximumError": "8kB"
                 }
               ],
               "outputHashing": "all"
             },
             "development": {
               "baseHref": "/",
               "deployUrl": "/",
               "optimization": false,
               "extractLicenses": false,
               "sourceMap": true
             }
           },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "options": {
            "port": 4200,
            "proxyConfig": "proxy.conf.json",
          },
          "configurations": {
            "production": {
              "buildTarget": "localemgmt-fe:build:production"
            },
            "development": {
              "buildTarget": "localemgmt-fe:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular/build:extract-i18n"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "tsConfig": "tsconfig.spec.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "public"
              }
            ],
            "styles": [
              "src/styles.css"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}
